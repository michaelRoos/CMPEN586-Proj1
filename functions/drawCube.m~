function [] = drawCube(corners, externalMatrix, internalMatrix)
%DRAWCUBE Summary of this function goes here
%   Detailed explanation goes here
faces = [1 2 3 4; 1 2 5 6; 1 4 5 8; 2 3 6 7; 5 6 7 8; 3 4 7 8; ];
cubeCentroid = mean(corners);
% distances = zeros(6,5);
% 
% for i = 1:6
%    face = faces(i,:);
%    distances(i,5) = i;
%    
%    z1 = norm(corners(face(1),:));
%    z2 = norm(corners(face(2),:));
%    z3 = norm(corners(face(3),:));
%    z4 = norm(corners(face(4),:));
%    distances(i,1:4) = sort([z1 z2 z3 z4]);
% end
% Zscore = sortrows(distances);
for i = 1:6
    face = faces(i,:);
    faceCorners = corners(face,:);
    faceCentroid = mean(faceCorners)
    faceNorm = faceCentroid - cubeCentroid;
    if dont(faceNorm, [0 0 1
    faceCorners = [corners(face(1),:); corners(face(2),:); corners(face(3),:); corners(face(4),:)];
    drawCubeSide(faceCorners, externalMatrix, internalMatrix, i);
   
end
end

