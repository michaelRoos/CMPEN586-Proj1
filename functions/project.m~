function [outputArg1,outputArg2] = project(points, externalMatrix, internalMatrix, position)
%PROJECT\ Summary of this function goes here
%   Detailed explanation goes here
projected = zeros(length(points),2)
for i = 1:length(points)
    point = points(i)
    projected = worldToCam(point(1),point(2),point(3), externalMatrix, internalMatrix, position)
end

